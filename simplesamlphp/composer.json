{
    "name": "mesh-research/knowledge-commons-simplesamlphp",
    "description": "Composer-managed SimpleSAMLphp",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/MESH-Research/simplesamlphp-module-idpsamlmdattributes.git"
        }
    ],
    "autoload": {
    },
    "authors": [
        {
            "name": "Mike Thicke",
            "email": "thickemi@msu.edu"
        }
    ],
    "require": {
        "simplesamlphp/simplesamlphp": "^2.0",
        "skoranda/simplesamlphp-module-idpsamlmdattributes" : "dev-master"
    },
    "config": {
        "allow-plugins": {
            "simplesamlphp/composer-module-installer": true
        },
        "platform-check": false
    }
}
